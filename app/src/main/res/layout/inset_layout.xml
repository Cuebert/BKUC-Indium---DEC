<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        android:id="@+id/inset_layout_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <View
        android:id="@+id/inset_layout_top"
        android:background="@color/lightThemeToolbar"
        android:layout_width="match_parent"
        android:layout_height="0dp"/>
    <View
        android:layout_gravity="right"
        android:id="@+id/inset_layout_right"
        android:background="@color/darkNavigationBarColor"
        android:layout_width="match_parent"
        android:layout_height="0dp"/>
    <View
        android:layout_gravity="bottom"
        android:id="@+id/inset_layout_bottom"
        android:background="@color/darkNavigationBarColor"
        android:layout_width="match_parent"
        android:layout_height="0dp"/>
</FrameLayout>
